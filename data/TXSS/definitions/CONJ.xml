<model inter_gene_max_space="30" min_mandatory_genes_required="2" min_genes_required="3" vers="2.0">  
	<gene name="T4SS_virb4" presence="mandatory">       
    		<exchangeables>
	            <gene name="T4SS_I_traU"/>
		</exchangeables>
	</gene>
	<gene name="T4SS_t4cp1" presence="mandatory">      
		<exchangeables>
			<gene name="T4SS_tcpA"/>
			<gene name="T4SS_t4cp2"/>
		</exchangeables>
	</gene>
	<gene name="T4SS_MOBB" presence="accessory" inter_gene_max_space="60">
		<exchangeables>
			<gene name="T4SS_MOBC" presence="accessory" inter_gene_max_space="60" />
			<gene name="T4SS_MOBF" presence="accessory" inter_gene_max_space="60" />
			<gene name="T4SS_MOBH" presence="accessory" inter_gene_max_space="60" />
			<gene name="T4SS_MOBP1" presence="accessory" inter_gene_max_space="60" />
			<gene name="T4SS_MOBP2" presence="accessory" inter_gene_max_space="60" />
			<gene name="T4SS_MOBP3" presence="accessory" inter_gene_max_space="60"/>
			<gene name="T4SS_MOBQ" presence="accessory" inter_gene_max_space="60" />
			<gene name="T4SS_MOBT" presence="accessory" inter_gene_max_space="60" />
			<gene name="T4SS_MOBV" presence="accessory" inter_gene_max_space="60" />
		</exchangeables>
	</gene>
</model>
